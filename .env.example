# Netlify Computer Use API Configuration
# Copy this file to .env for local development

# Required: Claude API Configuration
ANTHROPIC_API_KEY=your_claude_api_key_here

# Optional: Website Testing Configuration
WEBSITE_URL=app.giftround.com
MAX_TEST_DURATION=300

# Optional: Rate Limiting Configuration
MAX_REQUESTS_PER_HOUR=10
MAX_CONCURRENT_TESTS=1
BLOCK_DURATION_MINUTES=60

# Optional: Docker Configuration (for advanced setups)
DOCKER_HOST=/var/run/docker.sock
DOCKER_PORT=2375
# DOCKER_CA=path/to/ca.pem
# DOCKER_CERT=path/to/cert.pem
# DOCKER_KEY=path/to/key.pem

# Optional: Development Configuration
NODE_ENV=development